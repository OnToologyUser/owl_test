@prefix :        <http://www.semanticweb.org/chungho/ontologies/2017/2/untitled-ontology-28#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix swrl:    <http://www.w3.org/2003/11/swrl#> .
@prefix bim-owl-test:  <http://purl.org/net/bim-owl-test#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix swrlb:   <http://www.w3.org/2003/11/swrlb#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .

bim-owl-test:hasElements
      a       owl:InverseFunctionalProperty , owl:ObjectProperty ;
      rdfs:domain bim-owl-test:Building ;
      rdfs:range bim-owl-test:BuildingElement .

bim-owl-test:width
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger .

bim-owl-test:isConstructedAt
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain bim-owl-test:Building ;
      rdfs:range bim-owl-test:Site ;
      rdfs:subPropertyOf owl:topObjectProperty .

bim-owl-test:Column_2
      a       owl:NamedIndividual , bim-owl-test:Column ;
      bim-owl-test:isMadeBy
              bim-owl-test:Portland_Concrete .

bim-owl-test:height
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger ;
      rdfs:subPropertyOf owl:topDataProperty .

bim-owl-test:Column
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:StructuralElement , bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onProperty bim-owl-test:isMadeBy ;
                owl:someValuesFrom
                        [ a       owl:Class ;
                          owl:unionOf (bim-owl-test:Concrete bim-owl-test:Steel bim-owl-test:Wood)
                        ]
              ] .

bim-owl-test:isConnected
      a       owl:ObjectProperty , owl:SymmetricProperty ;
      rdfs:domain bim-owl-test:StructuralElement ;
      rdfs:range bim-owl-test:StructuralElement .

bim-owl-test:ConcreteElement
      a       owl:Class ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (bim-owl-test:BuildingElement [ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:isMadeBy ;
                            owl:someValuesFrom bim-owl-test:Concrete
                          ])
              ] .

bim-owl-test:Beam
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:StructuralElement , bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "1"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Materials ;
                owl:onProperty bim-owl-test:isMadeBy
              ] ;
      owl:equivalentClass
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "2"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Column ;
                owl:onProperty bim-owl-test:isConnected
              ] .

bim-owl-test:Wood
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Materials .

bim-owl-test:Factory_1
      a       bim-owl-test:Factory , owl:NamedIndividual .

bim-owl-test:Door
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "1"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Factory ;
                owl:onProperty bim-owl-test:hasBuiltAt
              ] ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "1"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Materials ;
                owl:onProperty bim-owl-test:isMadeBy
              ] .

bim-owl-test:Window_1
      a       owl:NamedIndividual , bim-owl-test:Window ;
      bim-owl-test:hasBuiltAt
              bim-owl-test:Factory_1 .

<urn:swrl#x>
      a       swrl:Variable .

bim-owl-test:Slab
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:StructuralElement , bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass bim-owl-test:Site ;
                owl:onProperty bim-owl-test:hasBuiltAt ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

bim-owl-test:position_X
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger ;
      rdfs:subPropertyOf owl:topDataProperty .

[]    a       owl:AllDifferent ;
      owl:distinctMembers (bim-owl-test:Column_1 bim-owl-test:Column_2) .

<urn:swrl#y>
      a       swrl:Variable .

bim-owl-test:Building
      a       owl:Class .

bim-owl-test:length
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger ;
      rdfs:subPropertyOf owl:topDataProperty .

bim-owl-test:Materials
      a       owl:Class .

bim-owl-test:material
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:string .

bim-owl-test:Site
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Location .

bim-owl-test:position_Y
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger ;
      rdfs:subPropertyOf owl:topDataProperty .

bim-owl-test:isMadeBy
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range bim-owl-test:Materials .

bim-owl-test:position_Z
      a       owl:FunctionalProperty , owl:DatatypeProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range xsd:nonNegativeInteger ;
      rdfs:subPropertyOf owl:topDataProperty .

[]    a       owl:AllDisjointProperties ;
      owl:members (bim-owl-test:height bim-owl-test:length bim-owl-test:material bim-owl-test:width) .

bim-owl-test:Glass
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Materials .

bim-owl-test:Door_1
      a       bim-owl-test:Door , owl:NamedIndividual ;
      bim-owl-test:hasBuiltAt
              bim-owl-test:Factory_1 .

bim-owl-test:SteelElement
      a       owl:Class ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (bim-owl-test:BuildingElement [ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:isMadeBy ;
                            owl:someValuesFrom bim-owl-test:Steel
                          ])
              ] .

bim-owl-test:Portland_Concrete
      a       owl:NamedIndividual , bim-owl-test:Concrete .

bim-owl-test:Location
      a       owl:Class ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:unionOf (bim-owl-test:Factory bim-owl-test:Site)
              ] .

bim-owl-test:InSituElement
      a       owl:Class ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (bim-owl-test:BuildingElement [ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:hasBuiltAt ;
                            owl:someValuesFrom bim-owl-test:Site
                          ])
              ] .

bim-owl-test:WoodElement
      a       owl:Class ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (bim-owl-test:BuildingElement [ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:isMadeBy ;
                            owl:someValuesFrom bim-owl-test:Wood
                          ])
              ] .

[]    a       swrl:Imp ;
      swrl:body
              [ a       swrl:AtomList ;
                rdf:first
                        [ a       swrl:IndividualPropertyAtom ;
                          swrl:argument1 <urn:swrl#x> ;
                          swrl:argument2 <urn:swrl#y> ;
                          swrl:propertyPredicate
                                  bim-owl-test:hasBuiltAt
                        ] ;
                rdf:rest
                        [ a       swrl:AtomList ;
                          rdf:first
                                  [ a       swrl:ClassAtom ;
                                    swrl:argument1 <urn:swrl#y> ;
                                    swrl:classPredicate bim-owl-test:Factory
                                  ] ;
                          rdf:rest
                                  [ a       swrl:AtomList ;
                                    rdf:first
                                            [ a       swrl:ClassAtom ;
                                              swrl:argument1 <urn:swrl#x> ;
                                              swrl:classPredicate bim-owl-test:Door
                                            ] ;
                                    rdf:rest ()
                                  ]
                        ]
              ] ;
      swrl:head
              [ a       swrl:AtomList ;
                rdf:first
                        [ a       swrl:ClassAtom ;
                          swrl:argument1 <urn:swrl#x> ;
                          swrl:classPredicate bim-owl-test:PrefabricatedElement
                        ] ;
                rdf:rest ()
              ] .

bim-owl-test:Concrete
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Materials .

bim-owl-test:StructuralElement
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:BuildingElement .

bim-owl-test:Steel
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Materials .

bim-owl-test:Factory
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Location .

bim-owl-test:Jobsite_1
      a       owl:NamedIndividual , bim-owl-test:Site .

bim-owl-test:BuildingElement
      a       owl:Class .

bim-owl-test:hasParent
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain
              [ a       owl:Class ;
                owl:unionOf (bim-owl-test:Door bim-owl-test:Window)
              ] ;
      rdfs:range bim-owl-test:Wall ;
      rdfs:subPropertyOf owl:topObjectProperty .

<http://purl.org/net/bim-owl-test>
      a       owl:Ontology .

bim-owl-test:Beam_1
      a       owl:NamedIndividual , bim-owl-test:Beam ;
      bim-owl-test:hasBuiltAt
              bim-owl-test:Jobsite_1 ;
      bim-owl-test:isConnected
              bim-owl-test:Column_1 , bim-owl-test:Column_2 ;
      bim-owl-test:isMadeBy
              bim-owl-test:Portland_Concrete .

bim-owl-test:PrefabricatedElement
      a       owl:Class ;
      owl:equivalentClass
              [ a       owl:Class ;
                owl:intersectionOf (bim-owl-test:BuildingElement [ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:hasBuiltAt ;
                            owl:someValuesFrom bim-owl-test:Factory
                          ])
              ] .

bim-owl-test:hasBuiltAt
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain bim-owl-test:BuildingElement ;
      rdfs:range bim-owl-test:Location .

bim-owl-test:Foundation
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:StructuralElement , bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Class ;
                owl:intersectionOf ([ a       owl:Restriction ;
                            owl:onProperty bim-owl-test:isMadeBy ;
                            owl:someValuesFrom bim-owl-test:Concrete
                          ] [ a       owl:Restriction ;
                            owl:onClass bim-owl-test:Site ;
                            owl:onProperty bim-owl-test:hasBuiltAt ;
                            owl:qualifiedCardinality
                                    "1"^^xsd:nonNegativeInteger
                          ])
              ] .

bim-owl-test:Wall
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:StructuralElement , bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:onClass bim-owl-test:Site ;
                owl:onProperty bim-owl-test:hasBuiltAt ;
                owl:qualifiedCardinality
                        "1"^^xsd:nonNegativeInteger
              ] .

bim-owl-test:isDeliveredTo
      a       owl:FunctionalProperty , owl:ObjectProperty ;
      rdfs:domain bim-owl-test:PrefabricatedElement ;
      rdfs:range bim-owl-test:Site ;
      rdfs:subPropertyOf owl:topObjectProperty .

bim-owl-test:Plastic
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:Materials .

bim-owl-test:Column_1
      a       owl:NamedIndividual , bim-owl-test:Column ;
      bim-owl-test:isMadeBy
              bim-owl-test:Portland_Concrete .

bim-owl-test:Window
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "1"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Factory ;
                owl:onProperty bim-owl-test:hasBuiltAt
              ] .

bim-owl-test:Facade
      a       owl:Class ;
      rdfs:subClassOf bim-owl-test:BuildingElement ;
      rdfs:subClassOf
              [ a       owl:Restriction ;
                owl:minQualifiedCardinality
                        "1"^^xsd:nonNegativeInteger ;
                owl:onClass bim-owl-test:Factory ;
                owl:onProperty bim-owl-test:hasBuiltAt
              ] .
